@page  "/models/create/"
@inject HttpClient _client
@inject NavigationManager _navManage

<h3 class="card-title">Create New Models</h3>
<hr />
<br />

<ModelsFormComponent ButtonText="Create Model" model="model" OnValidSubmit="@CreateModel" />

@code{
	Model model = new Model();

	private async Task CreateModel()
	{
		await _client.PostAsJsonAsync(Endpoints.ModelsEndpoint, model);
		_navManage.NavigateTo("/models/");
	}
}